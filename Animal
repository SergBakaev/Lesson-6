public abstract class Animal {
    protected String name;
    protected int _run;
    protected int _swin;

    public Animal(String name, int _run, int _swin){
        this.name = name;
        this._run = _run;
        this._swin = _swin;
    }

    public String getName() {
        return name;
    }
    public int getRun(){
        return _run;
    }
    public int getSwin(){
        return _swin;
    }
    abstract void run(int _run);
    abstract void swin(int _swin);
}